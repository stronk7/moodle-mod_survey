        <!-- <FIELD NAME="customnumber"     TYPE="char" LENGTH="64"     NOTNULL="false"                             SEQUENCE="false" PREVIOUS="contentformat"    NEXT="extrarow"/> -->
        <!-- <FIELD NAME="extrarow"         TYPE="int"  LENGTH="4"      NOTNULL="true"  UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="customnumber"     NEXT="extranote"/> -->
        <!-- <FIELD NAME="extranote"        TYPE="char" LENGTH="255"    NOTNULL="false"                             SEQUENCE="false" PREVIOUS="extrarow"         NEXT="required"/> -->
        <!-- <FIELD NAME="required"         TYPE="int"  LENGTH="4"      NOTNULL="false"                             SEQUENCE="false" PREVIOUS="extranote"        NEXT="hideinstructions"/> -->
        <!-- <FIELD NAME="hideinstructions" TYPE="int"  LENGTH="4"      NOTNULL="false"                             SEQUENCE="false" PREVIOUS="required"         NEXT="variable"/> -->
        <!-- <FIELD NAME="variable"         TYPE="char" LENGTH="64"     NOTNULL="false"                             SEQUENCE="false" PREVIOUS="hideinstructions" NEXT="indent"/> -->
        <!-- <FIELD NAME="indent"           TYPE="int"  LENGTH="4"      NOTNULL="false"                             SEQUENCE="false" PREVIOUS="variable"         NEXT="parentid"/> -->
        <!-- end of fields belonging to itembase_form.php -->

        <!-- begin of fields belonging to plugin_form.php of this specific plugin -->
        <!-- end of fields belonging to plugin_form.php of this specific plugin -->
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="surveyid" UNIQUE="false" FIELDS="surveyid" NEXT="itemid"/>
        <INDEX NAME="itemid"   UNIQUE="true"  FIELDS="itemid"   PREVIOUS="surveyid"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>
